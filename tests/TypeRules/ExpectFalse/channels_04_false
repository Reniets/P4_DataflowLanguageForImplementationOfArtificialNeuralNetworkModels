block channels_04 {
    mychannel:in in;
	mychannel:out out;

	blueprint() {
	    mychannel:in in_1 = this.in;
	    mychannel:out out_1 = this.out;

	    block b = build Test();

        out_1 -> b.out;
        b.in -> this.out;
	}
}

block Test {
    mychannel:in in;
    mychannel:out out;

    blueprint() {
        this.in -> this.out;
    }
}