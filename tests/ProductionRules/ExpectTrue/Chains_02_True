// Test of using channels in chains.
block chain_02 {
	mychannel:in input;
	mychannel:out output;
	
	blueprint() {
		block a = build Test();
		block b = build Test();
		block c = build Test();
		
		this.input -> a -> b -> c -> this.output;
	}
}

block Test {
    mychannel:out output;
    mychannel:in input;
    blueprint() {
    }
}