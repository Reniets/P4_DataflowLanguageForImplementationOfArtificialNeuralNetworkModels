}

// dot gate
block decla_09 {
	gate:in input;
	gate:out output;
	
	blueprint() {
		block b = build Test();
	
		gate:in s = (b.input);
	}
}

// dot gate
block decla_10 {
	gate:in input;
	gate:out output;
	
	blueprint() {
		block b = build Test();
		b = build Abc();
	
		gate:in s = (this.input);
	}
}

