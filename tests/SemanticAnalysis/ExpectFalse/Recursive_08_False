block Main {
    mychannel:in in;
    mychannel:out out;

    blueprint() {
        block master = build recursive_02();
    }
}

block recursive_02 {
    mychannel:in in;
	mychannel:out out;

	blueprint() {
        in -> build recursive_02_2() -> out;
	}
}

block recursive_02_2 {
    mychannel:in in;
	mychannel:out out;

	blueprint() {
        in -> build recursive_02() -> out;
	}
}