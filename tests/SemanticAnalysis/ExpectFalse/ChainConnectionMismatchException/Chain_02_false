block Master {
    mychannel:in in;
    mychannel:out out;

    blueprint() {
        (this.in, this.in) -> build Slave() -> this.out;
    }
}

block Slave {
    mychannel:in in1;
    mychannel:in in2;
    mychannel:out out;

    blueprint() {
        operation mult = build Multiplication();

        (this.in1) -> mult;
        this.in2 -> this.out;
    }
}