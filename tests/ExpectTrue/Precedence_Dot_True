// Test of simple multi chaining.
block chain_01 {
	mychannel:in input;
	mychannel:out output;
	
	blueprint() {
		block a = build Test();
		block b = build Test();
		block c = build Test();
		
		this.input -> a = b -> b -> c -> this.output;
	}
}

block Test {
    mychannel:in input;
    mychannel:out output;

    blueprint() {
        this.input -> this.output;
    }
}