block Main {
    mychannel:in in;
    mychannel:out out;

    blueprint () {
        blueprint param = draw B1;

        proc(param, param);
    }

    procedure proc(blueprint blue1, blueprint blue2) {
        this.in -> build blue1() -> build blue2() -> this.out;
    }
}

block B1 {
    mychannel:in in;
    mychannel:out out;

    blueprint () {
        this.in -> this.out;
    }
}