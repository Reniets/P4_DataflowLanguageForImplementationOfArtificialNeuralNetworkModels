// Test of using channels in chains.
block chain_02 {
	channel:in input;
	channel:out output;
	
	blueprint() {
		block a = build Test();
		block b = build Test();
		block c = build Test();
		
		this.input -> a -> b -> c -> this.output;
	}
}

block Test {
    channel:out output;
    channel:in input;
    blueprint() {
    }
}