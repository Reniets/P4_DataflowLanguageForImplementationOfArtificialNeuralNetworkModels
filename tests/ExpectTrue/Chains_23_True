// Test of simple multi chaining.
 block chain_01 {
 	mychannel:in input;
 	mychannel:out output;

 	blueprint() {
 		block a;

 		this.input -> a = build Test();
 		a.output -> this.output;
 	}
 }

 block Test {
     mychannel:in input;
     mychannel:out output;

     blueprint() {
         this.input -> this.output;
     }
 }