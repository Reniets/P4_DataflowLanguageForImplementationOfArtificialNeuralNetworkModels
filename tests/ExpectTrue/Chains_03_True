// Test of multiple small chains.
block chain_03 {
	mychannel:in input;
	mychannel:out output;
	
	blueprint() {
		block a = build Test();
		block b = build Test();
		block c = build Test();
		
		this.input -> a;
		a -> b;
		b -> c;
		c -> this.output;
	}
}

block Test {
    mychannel:in input;
    mychannel:out output;

    blueprint() {
        this.input -> this.output;
    }
}