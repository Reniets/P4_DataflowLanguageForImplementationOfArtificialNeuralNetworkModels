// Test of multiple small chains.
block chain_03 {
	channel:in input;
	channel:out output;
	
	blueprint() {
		block a = build Test();
		block b = build Test();
		block c = build Test();
		
		this.input -> a;
		a -> b;
		b -> c;
		c -> this.output;
	}
}

block Test {
    channel:out output;
    channel:in input;
