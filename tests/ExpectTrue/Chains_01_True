// Test of simple multi chaining.
block chain_01 {
	mychannel:in input;
	mychannel:out output;
	
	blueprint() {
		block a = build Test();
		block b = build Test();
		block c = build Test();
		
		a -> b -> c;
	}
}

block Test {
    mychannel:out output;
    mychannel:in input;
    blueprint() {
    }
}