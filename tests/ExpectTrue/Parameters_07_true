block Main {
    mychannel:in in;
    mychannel:out out;

    blueprint () {
        blueprint param = draw B2;

        this.in -> build B1(B2) -> build B2(draw B3) -> this.out;
    }
}

block B1 {
    mychannel:in in;
    mychannel:out out;

    blueprint (blueprint ToBuild) {
        this.in -> build ToBuild(draw B3) -> this.out;
    }
}

block B2 {
    mychannel:in in;
    mychannel:out out;

    blueprint (blueprint ToBuild) {
        this.in -> build ToBuild() -> this.out;
    }
}

block B3 {
    mychannel:in in;
    mychannel:out out;

    blueprint () {
        this.in -> this.out;
    }
}
