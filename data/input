block AnnLayer {
    mychannel:in in;
    mychannel:out out;

    blueprint(){
        this.in -> build Layer(draw _Sigmoid, draw _Relu) -> this.out;
    }
}

block Layer {
    mychannel:in in;
    mychannel:out out;

    blueprint(blueprint helloWorld, blueprint hmm){
        operation myOpe = build helloWorld();

        this.in -> myOpe -> build hmm() -> this.out;
    }
}