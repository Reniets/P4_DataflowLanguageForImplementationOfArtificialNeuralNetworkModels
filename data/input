block AnnLayer {
    mychannel:in in;
    mychannel:out out;

    blueprint(){
        mychannel:in input = this.in;
        mychannel:out output = this.out;

        operation ope = build Multiplication();

        channel:in oIn = ope.A;
        channel:out oOut = ope.out;

        myProd(build _Sigmoid());
    }

    procedure myProd(operation myBlock) {
        this.in -> myBlock -> this.out;
    }
}