block AnnLayer {
    mychannel:in in;
    mychannel:out out;

    blueprint(){
        blueprint julla = draw _Sigmoid;
    }

    procedure myProd(blueprint myOpe) {
        operation abc = build myOpe();

        this.in -> abc -> this.out;
    }

    procedure myProd2(blueprint myOpe) {
        this.in -> build myOpe() -> this.out;
    }
}