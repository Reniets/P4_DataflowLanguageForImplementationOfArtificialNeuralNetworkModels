block ANN {
    mychannel:in in;
    mychannel:out out;

    blueprint() {
        this.in -> build Layer(draw Layer2) -> this.out;
    }
}

block Layer {
    mychannel:in in;
    mychannel:out out;

    blueprint(blueprint blue) {
        this.in -> build blue(draw Test) -> this.out;
    }
}

block Layer2 {
    mychannel:in in;
    mychannel:out out;

    blueprint(blueprint blue) {
        this.in -> build blue() -> this.out;
    }
}

block Test {
    mychannel:in in;
    mychannel:out out;

    blueprint() {
        this.in -> this.out;
    }
}